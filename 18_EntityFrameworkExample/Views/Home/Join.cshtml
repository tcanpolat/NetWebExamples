@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
</div>
<table class="table">
    <thead>
        <tr>
            <th>
                Öğrenci Adı
            </th>
            <th>
                Kurs Adı
            </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.StudentName
                </td>
                <td>
                    @item.CourseTitle
                </td>
                
            </tr>
        }


    </tbody>

</table>
